{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Contour",
	"patterns": [
	  {
		"include": "#keywords"
	  },
	  {
		"include": "#strings"
	  }
	],
	"repository": {
	  "keywords": {
		"patterns": [
		  {
			"name": "keyword.control.contour",
			"match": "\\b(if|while|for|return|switch|case|break|continue)\\b"
		  },
		  {
			"name": "keyword.declaration.contour",
			"match": "\\b(var|let|const|function|class|struct|interface|typedef|typealias)\\b"
		  },
		  {
			"name": "keyword.access.contour",
			"match": "\\b(layer)\\b"
		  },
		  {
			"name": "keyword.exception.contour",
			"match": "\\b(try|catch|finally|throw|throws)\\b"
		  },
		  {
			"name": "keyword.contextual.contour",
			"match": "\\b(partial|override|virtual|abstract|sealed)\\b"
		  },
		  {
			"name": "keyword.type.contour",
			"match": "\\b(typeof|instanceof|is|as)\\b"
		  },
		  {
			"name": "keyword.modifier.contour",
			"match": "\\b(static|final|readonly|volatile|inline)\\b"
		  },
		  {
			"name": "keyword.other.contour",
			"match": "\\b(result|uses|complete|import|export|package|namespace|new|delete|this|super|extends|implements|true|false|null|undefined)\\b"
		  }
		]
	  },
	  "strings": {
		"name": "string.quoted.double.contour",
		"begin": "\"",
		"end": "\"",
		"patterns": [
		  {
			"name": "constant.character.escape.contour",
			"match": "\\\\."
		  }
		]
	  }
	},
	"scopeName": "source.contour"
  }
  